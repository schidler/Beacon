<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Flash" >

    <LinearLayout
        android:id="@+id/linear1"
        android:layout_width="match_parent"
        android:layout_height="102dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/view"
            android:layout_width="106dp"
            android:layout_height="73dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="10dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear2"
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/linear3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/set_red"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Red" />

            <Button
                android:id="@+id/set_green"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Green" />

            <Button
                android:id="@+id/set_blue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Blue" />

            <Button
                android:id="@+id/set_white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="White" />

            <Button
                android:id="@+id/set_yellow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Yellow" />

            <Button
                android:id="@+id/set_cyan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="cyan" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/start_down_time"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:text="StartDownTime:" />

            <EditText
                android:id="@+id/start_down_time_edit"
                android:layout_width="78dp"
                android:layout_height="wrap_content"
                android:text="" >
            </EditText>

            <Button
                android:id="@+id/start_up_time"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_marginRight="10dp"
                android:text="StartUpTime:" />

            <EditText
                android:id="@+id/start_up_time_edit"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="1.16"
                android:text="" >
            </EditText>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/high_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="HighTime:" />

            <EditText
                android:id="@+id/high_time_edit"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:text="" >
            </EditText>

            <Button
                android:id="@+id/low_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="LowTime:" />

            <EditText
                android:id="@+id/low_time_edit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="" >
            </EditText>
        </LinearLayout>

        <EditText
            android:id="@+id/edit"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:text="" >
        </EditText>

        <LinearLayout
            android:id="@+id/linear5"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <Button
                android:id="@+id/set_frq"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Set up" />

            <Button
                android:id="@+id/button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:focusable="true"
                android:text="Turn" />

            <ToggleButton
                android:id="@+id/toggle"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="On" />

        </LinearLayout>
    </LinearLayout>

    <EditText
        android:id="@+id/start_up_time_edit"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:ems="10" >

        <requestFocus />
    </EditText>

</LinearLayout>